public class HwPartTwo {
    public static void main(String[] args) {
        int[] someArray = {5,2,1,3,4,1,5};
        sumIntegers(someArray);
    }
    public static void sumIntegers (int[] arr) {
        int sum = 0;
        for (int i = 0; i < arr.length; i++) {
            sum = sum + arr[i];
        }
        System.out.println(sum);
    }
}
